@keyframes rotating {
    from {
        transform: rotate(0deg);

    }
    to {
        transform: rotate(360deg);

    }
}
@keyframes scaling {
    from {
        transform: scale(0);

    }
    to {
        transform: scale(1);

    }
}

#confetti-holder{
    width: 100vw;
    height 100vh;
    z-index:99999;
    background: none;
    position: absolute;
    top:0;
    left:0;
    pointer-events: none;
}
.click-confetti{
    position: absolute;
    width: var(--click-width, 300px) !important;
    height:  var(--click-height, 200px) !important;
    //top: calc(var(--click-height, 200px) / 3)
    //left: calc(var(--click-width, 300px) / -3);
    top: calc(var(--click-y) + calc((var(--click-height)/2)))
    left: calc(var(--click-x) - calc(var(--click-width)/2))
    z-index 999;
    transform: none !important;
    svg{
        width: var(--click-width, 300px) !important;
        height:  var(--click-height, 200px) !important;
        transform: none !important;
        position: absolute;
        top:0;
        left:0;
        z-index 999;
      
    }
}

#app.tinker #confetti-holder .click-confetti{
    path{
        stroke:rgb(241,103,100) !important;
    }
}
#app.explorer #confetti-holder .click-confetti{
    path{
        stroke:rgb(255, 152, 0) !important;
    }
}
#app.developer #confetti-holder .click-confetti{
    path{
        stroke:#46738D !important;
    }
}
#app.animator #confetti-holder .click-confetti{
    path{
        stroke:#502455 !important;
    }
}


